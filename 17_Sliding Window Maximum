239. Sliding Window Maximum

class Solution:
    def maxSlidingWindow(self, nums: List[int], k: int) -> List[int]:
        n = len(nums)
        dq = deque()
        res = []
        for i in range(n):
            # Remove last window element
            if dq and dq[0] <= i - k:
                dq.popleft()
            #Move next element
            while dq and nums[dq[-1]] <= nums[i]:
                dq.pop()
            dq.append(i) 

            #append max element of window
            if i >= k-1:
                res.append(nums[dq[0]])
        return res
