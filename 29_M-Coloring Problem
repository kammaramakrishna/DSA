M-Coloring Problem
# User function Template for python3

class Solution:
    def isSafe(self, node, color, col, v, edges):
        for i in range(v):
            #not self node, check adjecent, check for already colored
            if i != node and edges[i][node] == 1 and color[i] == col:
                return False
        return True
        
    def solve(self, node, color, m, v, edges):
        if node == v:
            return True
        for i in range(1, m+1):
            if self.isSafe(node, color, i, v, edges):
                color[node] = i
                if self.solve(node + 1, color, m, v, edges):
                    return True
                color[node] = 0
        return False
    
    def graphColoring(self, v, edges, m):
        # code here
        #convert list of list into adjecent matrix
        adj = [[0]*v for _ in range(v)]
        for i,j in edges:
            adj[i][j] = 1
            adj[j][i] = 1
        color = [0] * v
        if self.solve(0, color, m, v, adj):
            return True
        else:
            return False
