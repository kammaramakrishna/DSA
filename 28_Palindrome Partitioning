131. Palindrome Partitioning

class Solution:
    def partition(self, s: str) -> List[List[str]]:
        res = []
        path = []
        self.backtrack(0, s, res, path)
        return res
    def backtrack(self, ind, s, res, path):
        if ind == len(s):
            res.append(path[:])
            return 
        
        for i in range(ind, len(s)):
            if self.isPalindrome(ind, s, i):
                path.append(s[ind:i+1])
                self.backtrack(i+1, s, res, path)
                path.pop()
    def isPalindrome(self, left, s, right):
        # while left < right:
        #     if s[left] != s[right]:
        #         return False
        #     left += 1
        #     right -= 1
        # return True
        x = s[left: right + 1]
        if x == x[::-1]:
            return True
        else:
            return False
        
