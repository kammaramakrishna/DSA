        #     while current:
        #         count += 1
        #         current = current.next
        #     return count
        
        # n = length(head)
        # n = n // 2
        # count = 0
        # current = head
        # while count < (n - 1):
        #     current = current.next
        #     count += 1
        # current.next = current.next.next
        # return head



        
